{
  "name": "challenges",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "yargs": "^17.5.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn",
      "react-hooks/exhaustive-deps": "off",
      "react/jsx-filename-extension": [
        0
      ],
      "import/extensions": "off",
      "prettier/prettier": [
        "warn"
      ]
    },
    "ignorePatterns": [
      "src/__test__/*"
    ]
  }
}